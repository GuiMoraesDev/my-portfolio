{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@tanstack/query"],
  "rules": {
    "no-console": "error",
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ],
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": { "order": "asc", "caseInsensitive": true }
      }
    ]
  }
}
